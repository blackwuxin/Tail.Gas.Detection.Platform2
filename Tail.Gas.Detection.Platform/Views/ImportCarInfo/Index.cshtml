<style type="text/css">
    td {
        word-wrap: break-word;
        word-break: break-all;
        white-space: normal;
    }
</style>
<script type="text/javascript">
	seajs.use('jquery/jquery.ui/1.11.0/jquery-ui.css');
	seajs.use('jquery/jquery.ui/1.11.0/jquery-ui-timepicker-addon.css');
	seajs.use(['jquery', 'jquery.ui', 'jquery.ui.timepicker'], function ($, jui, juaddon) {
		$(document).ready(function ($) {
			$('#start-datetime').datetimepicker({ dateFormat: "yy/mm/dd", timeFormat: "HH:mm:ss" });
			$('#end-datetime').datetimepicker({ dateFormat: "yy/mm/dd", timeFormat: "HH:mm:ss" });
		});
	});
</script>
<form id="search_form" class="ng-pristine ng-valid" method="post">
    <div class="col-md-12">
        <div class="form-group col-md-2">
            <input id="btn_import" type="button" class="btn btn-primary btn-md" value="导入车辆信息" />
        </div>
        <div class="form-group  col-md-2">
            <input id="btn_download" type="button" class="btn btn-primary btn-md" onclick="window.location.href = '../filedemo/carinfodemo.xls'" value="标准模板下载" />
            @*<a href="filedemo/carinfodemo.xls" target="_self">标准模板下载</a>*@
        </div>

    </div>
    <div class="col-md-12" >
        <div class="form-group col-md-2">
            <label class="col-sm-4 control-label">车牌号：</label>
            <div class="col-sm-8">
                <input type="text" id="CarNo" class="form-control ng-pristine ng-valid" placeholder="车牌号">
            </div>
        </div>
        <div class="form-group col-md-2">
            <label class="col-sm-4 control-label">车辆归属地：</label>
            <div class="col-sm-8">
                <select id="Belong" name="Belong" class="form-control ng-pristine ng-valid" ng-model="queryObj.appCategoryId" ng-options="cat.id as cat.name for cat in appCategories"></select>
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="col-sm-3 control-label">开始时间：</label>
            <div class="col-sm-9">
                <input type="text" id="start-datetime" class="form-control ng-pristine ng-valid" placeholder="开始时间">
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="col-sm-3 control-label">结束时间：</label>
            <div class="col-sm-9">
                <input type="text" id="end-datetime" class="form-control ng-pristine ng-valid" placeholder="结束时间">
            </div>
        </div>
        <div class="form-group col-md-2">
            <div class="form-group col-md-4" style="text-align: right;">
                <input id="btn-search" type="button" class="btn btn-primary btn-sm search-btn" value="查询" />
            </div>
        </div>
    </div>
</form>


<div style="margin-top: 20px;" >
    <table id="pclist" class="table table-striped table-bordered  table-hover" cellspacing="0">
        <thead>
            <tr style="background-color: #337ab7; color: white;">
                <th width="9%" class="text-center">车牌</th>
                <th width="9%" class="text-center">车辆颜色</th>
                <th width="9%" class="text-center">车辆类型</th>
                <th width="9%" class="text-center">车辆归属地</th>
                <th width="9%" class="text-center">原始排放</th>
                <th width="9%" class="text-center">匹配产品型号</th>
                <th width="9%" class="text-center">改装公司</th>
                <th width="9%" class="text-center">车主名字</th>
                <th width="9%" class="text-center">个人/公司</th>
                <th width="9%" class="text-center">联系方法</th>
                <th width="9%" class="text-center">改造完成时间</th>
            </tr>
        </thead>

        <tbody></tbody>
    </table>
</div>
<div class="modal fade upload" id="upload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">上传文件</h4>
            </div>
            <div class="modal-body">
                <form id="uploadform" class="ke-upload-area ke-form" method="post" enctype="multipart/form-data" target="upfileiframe" action="../importcarinfo/upfile" style="width: 0px;">
                    <input type="file" id="uploadfileinput" class="ke-upload-file" name="messagecodeFile" tabindex="-1">
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" id="btn-uploadfile" class="btn btn-primary" data-dismiss="modal">确定</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
            </div>
        </div>
    </div>
</div>
<iframe name="upfileiframe" style="display:none;" src="javascript:false" id="upfileiframe"></iframe>
<script type="text/javascript">
    seajs.use(['$', 'importcarinfo'], function ($, importcarinfo) {;
        $(document).ready(function () {
            importcarinfo.init();
                     
        });
    });
</script>
