<style type="text/css">
    td {
        word-wrap: break-word;
        word-break: break-all;
        white-space: normal;
    }
</style>
<script type="text/javascript">
    seajs.use('jquery/jquery.ui/1.11.0/jquery-ui.css');
    seajs.use('jquery/jquery.ui/1.11.0/jquery-ui-timepicker-addon.css');
    seajs.use(['jquery', 'jquery.ui', 'jquery.ui.timepicker'], function ($, jui, juaddon) {
        $(document).ready(function ($) {
            $('#start-datetime').datetimepicker({ dateFormat: "yy/mm/dd", timeFormat: "HH:mm:ss" });
            $('#end-datetime').datetimepicker({ dateFormat: "yy/mm/dd", timeFormat: "HH:mm:ss" });
        });
    });
</script>
<form id="search_form" class="ng-pristine ng-valid" method="post">

    <div class="col-md-12">
        <div class="form-group col-md-2">
            <label class="col-sm-4 control-label">车牌号：</label>
            <div class="col-sm-8">
                <input type="text" id="CarNo" class="form-control ng-pristine ng-valid" placeholder="车牌号">
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="col-sm-3 control-label">开始时间：</label>
            <div class="col-sm-9">
                <input type="text" id="start-datetime" class="form-control ng-pristine ng-valid" placeholder="开始时间">
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="col-sm-3 control-label">结束时间：</label>
            <div class="col-sm-9">
                <input type="text" id="end-datetime" class="form-control ng-pristine ng-valid" placeholder="结束时间">
            </div>
        </div>
        <div class="form-group col-md-2">
            <div class="form-group col-md-4" style="text-align: right;">
                <input id="btn-search" type="button" class="btn btn-primary btn-sm search-btn" value="查询" />
            </div>
        </div>
    </div>
</form>
<div id="allmap"></div>

<script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&ak=q2HUo15ZyO2CB7fZ90n0p5gxFVRGOmZa"></script>

<script type="text/javascript">
    seajs.use(['$', 'mappath'], function ($, path) {;
        $(document).ready(function () {
            path.init();

        });
    });
</script>

