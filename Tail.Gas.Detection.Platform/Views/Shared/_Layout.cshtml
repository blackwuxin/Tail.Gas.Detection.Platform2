<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>机动车尾气排放监控系统平台</title>
    <link href="@ViewBag.ApplicationPath/Assets/sea-modules/app/app/1.0.0/cuio-v1.1.css" rel="stylesheet" />
    <link href="@ViewBag.ApplicationPath/Assets/sea-modules/extend/bootstrap/3.3.2/css/bootstrap.css" rel="stylesheet" />
    <link href="@ViewBag.ApplicationPath/Assets/sea-modules/extend/bootstrap/3.3.2/main.css" rel="stylesheet" />
    <link href="@ViewBag.ApplicationPath/Assets/sea-modules/jquery/dataTables/1.10.4/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="@ViewBag.ApplicationPath/Assets/sea-modules/extend/bootstrapdialog/3.3.2/bootstrap-dialog.css" rel="stylesheet" />
    <script type="text/javascript" src="@ViewBag.ApplicationPath/Assets/sea-modules/seajs/seajs/2.3.0/sea.js" id="seajsnode"></script>
    <script type="text/javascript" src="@ViewBag.ApplicationPath/Assets/sea-modules/seajs/seajs/2.3.0/seajs-css.js" id="seajsnode"></script>
    <script type="text/javascript" src="@ViewBag.ApplicationPath/Assets/sea-modules/sea-config.js"></script>
    <script type="text/javascript">
        seajs.config({
            base: '@ViewBag.ApplicationPath/assets'
        });

        window.BASE_PATH = '@ViewBag.ApplicationPath';

        //初始化工具栏
        @Html.Raw(@ViewBag.ToolKit)

        //注销
        seajs.use('jquery', function ($) {
            $(document).ready(function () {
                $("#logout").click(function () {
                    $.ajax({
                        type: "POST",
                        url: window.BASE_PATH + "/base/logout",
                        data: {},
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            if (data) {
                                if (data.result == "success") {
                                    window.location = window.BASE_PATH + "/home";
                                } else {
                                    console.log(data);
                                }
                            }
                        },
                        error: function (err) {
                            console.log(err);
                        }
                    });
                });
            });
        })



    </script>
</head>
<body>
    <header>
        <div class="hd">
            <div class="hd-wrapper">
                @*<h1 class="logo"><a href="" title="机动车尾气排放监控平台">机动车尾气排放监控平台</a></h1>*@
